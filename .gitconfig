[include]
    path = ~/.local/config/gitconfig
[push]
    default = simple
[alias]
    co = checkout
    ci = commit
    st = status
    fe = fetch
    br = branch
    pl = pull
    df = diff
    hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
    type = cat-file -t
    dump = cat-file -p
    ls = log --oneline -n 20 --graph
[core]
    excludesfile = /home/bart/.gitignore_global
[credential]
    helper = store
[gitlab]
	user = bartfrenk
