[TYPECHECK]
ignored-modules=numpy, tensorflow, pydantic
ignored-classes=scoped_session, BaseModel

[MESSAGES CONTROL]
disable=missing-docstring,
        duplicate-code,
        invalid-name,
        too-few-public-methods,
        redefined-builtin,
        invalid-name,
        redefined-outer-name

[SIMILARITIES]
ignore-imports=yes

[MASTER]
ignore=versions
extension-pkg-whitelist=pydantic
unsafe-load-any-extension=y

[MISCELLANEOUS]
notes=FIXME,XXX